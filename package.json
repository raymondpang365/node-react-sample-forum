{
  "name": "react-cool-starter",
  "version": "2.3.0",
  "description": "A forum made with react and nodejs",
  "main": "index.js",
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/psfr937/react-nodejs-sample-forum.git"
  },
  "keywords": [
    "starter",
    "boilerpate",
    "universal",
    "react",
    "redux",
    "react router v4",
    "express",
    "webpack v3",
    "es6",
    "flow",
    "code splitting",
    "hot reloading",
    "babel",
    "postcss",
    "jest",
    "unit testing",
    "performance optimizing"
  ],
  "author": "Raymond Pang <psfr937@gmail.com> ",
  "license": "MIT",
  "homepage": "https://github.com/psfr937/react-nodejs-sample-forum.git",
  "scripts": {
    "start": "better-npm-run start",
    "start:dashboard": "webpack-dashboard yarn start",
    "start:production": "yarn build && yarn start:prod",
    "start:prod": "better-npm-run start:prod",
    "build": "yarn clean:build && better-npm-run build",
    "build:stats": "yarn clean:build && better-npm-run build:stats",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:js": "better-npm-run lint:js",
    "lint:style": "better-npm-run lint:style",
    "flow": "better-npm-run flow",
    "flow:stop": "better-npm-run flow:stop",
    "test": "better-npm-run test",
    "test:watch": "yarn test --watch",
    "clean:all": "yarn clean:build && yarn clean:test",
    "clean:build": "better-npm-run clean:build",
    "clean:test": "better-npm-run clean:test",
    "coveralls": "better-npm-run coveralls && yarn clean:test"
  },
  "betterScripts": {
    "start": {
      "command": "nodemon ./index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "NODE_HOST": "localhost",
        "LISTEN_TO": "localhost",
        "PORT": 3000
      }
    },
    "start:prod": {
      "command": "node ./index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "build": {
      "command": "webpack --progress --hide-modules --config ./tools/webpack/config.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:stats": {
      "command": "webpack --progress --hide-modules --config ./tools/webpack/config.babel.js",
      "env": {
        "NODE_ENV": "stats"
      }
    },
    "lint:js": {
      "command": "eslint --fix ./src ./tools ./index.js"
    },
    "lint:style": {
      "command": "stylelint --fix \"./src/**/*.scss\""
    },
    "flow": {
      "command": "flow"
    },
    "flow:stop": {
      "command": "flow stop"
    },
    "test": {
      "command": "jest --coverage",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "clean:build": {
      "command": "rimraf ./public/assets"
    },
    "clean:test": {
      "command": "rimraf ./coverage"
    },
    "coveralls": {
      "command": "cat ./coverage/lcov.info | coveralls"
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "useBuiltIns": true
        }
      ],
      "react",
      "stage-0",
      "flow"
    ],
    "plugins": [
      "loadable-components/babel",
      "dynamic-import-node",
      "lodash"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "plugin:lodash-fp/recommended",
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "lodash-fp",
      "react",
      "jsx-a11y",
      "import",
      "prettier"
    ],
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./tools/webpack/config.babel.js"
        }
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "rules": {
      "react/no-unused-state": 0,
      "preder-destructuring": 0,
      "no-shadow": 0,
      "no-throw-literal": 0,
      "camelcase": 0,
      "lodash-fp/prefer-constant": 0,
      "preder-default-export": 0,
      "consistent-return": 0,
      "func-names": 0,
      "no-alert": 0,
      "react/require-default-props": 0,
      "jsx-a11y/label-has-for": 0,
      "comment-whitespace-inside": 0,
      "at-rule-name-space-after": 0,
      "no-else-return": 0,
      "no-unused-vars": 0,
      "class-methods-use-this": 0,
      "no-unused-expressions": [
        2,
        {
          "allowShortCircuit": true,
          "allowTernary": true
        }
      ],
      "no-constant-condition": 0,
      "array-callback-return": 0,
      "import/no-dynamic-require": 0,
      "no-nested-ternary": 0,
      "react/jsx-no-bind": 0,
      "guard-for-in": 0,
      "no-restricted-syntax": 0,
      "no-param-reassign": 0,
      "react/prop-types": 0,
      "react/forbid-prop-types": 0,
      "global-require": 0,
      "no-underscore-dangle": 0,
      "no-console": 0,
      "function-paren-newline": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "jsx-a11y/anchor-is-valid": [
        0,
        {
          "specialLink": [
            "to"
          ]
        }
      ],
      "lodash-fp/use-fp": 0,
      "lodash-fp/consistent-name": 0,
      "prettier/prettier": [
        0,
        {
          "singleQuote": true
        }
      ]
    },
    "globals": {
      "__CLIENT__": true,
      "__SERVER__": true,
      "__DEV__": true
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "./node_modules/prettier-stylelint/config.js"
    ],
    "plugins": [
      "stylelint-scss"
    ],
    "rules": {
      "scss/selector-no-redundant-nesting-selector": true,
      "string-quotes": "single",
      "at-rule-no-unknown": null,
      "scss/at-rule-no-unknown": true,
      "comment-whitespace-inside": null,
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global",
            "local"
          ]
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 11",
    "safari >= 7"
  ],
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/tools/jest/setup.js"
    ],
    "globals": {
      "__DEV__": true
    },
    "collectCoverageFrom": [
      "src/containers/App/App.js",
      "src/containers/Home/Home.js",
      "src/containers/UserInfo/UserInfo.js",
      "src/containers/NotFound/NotFound.js",
      "src/components/**/*.js",
      "src/actions/**/*.js",
      "src/reducers/**/*.js",
      "!src/reducers/index.js",
      "!src/**/__tests__"
    ],
    "moduleNameMapper": {
      ".*\\.(css|scss|sass)$": "<rootDir>/tools/jest/styleMock.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/jest/assetMock.js"
    }
  },
